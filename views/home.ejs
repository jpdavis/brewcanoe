    <body>
        <div class="container">
            <% if(error && error.length > 0){ %>
                <div class="alert alert-danger" role="alert">
                    <%= error %>
                </div>
            <% } %>
            <% if(success && success.length > 0){ %>
                <div class="alert alert-success" role="alert">
                    <%= success %>
                </div>
            <% } %>
        </div>

        <div id="landing-header">
		<img src="../media/landing/logo3.png">
		<h2><strong>Please bear with us as we migrate to a new e-commerce platform.</strong></h2>
    		<a href="https://blog.brewcanoe.ca" class="btn btn-lg">Meanwhile, check out our blog!</a>
        </div>
        
